{
    "name": "Tempt",
    "version": "1.0",
    "description": "Build a productivity extension!",
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_popup": "popUp.html",
    },
    "commands": {
      "_execute_browser_action": {
        "suggested_key": {
          "default": "Ctrl+Shift+F",
          "mac": "MacCtrl+Shift+F"
        },
    "manifest_version": 2
  }
